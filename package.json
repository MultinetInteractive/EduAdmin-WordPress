{
	"name": "eduadmin-wordpress-plugin",
	"private": true,
	"version": "1.0.0",
	"repository": "https://github.com/MultinetInteractive/EduAdmin-WordPress.git",
	"author": "Chris GÃ¥rdenberg <chga@multinet.se>",
	"license": "MIT",
	"scripts": {
		"build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o content/style/compiled",
		"build-task:autoprefixer": "postcss content/style/compiled/*.css --use autoprefixer -d content/style/compiled",
		"sass:build": "npm-run-all -p build-task:*",
		"sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
		"dev": "npm-run-all -p sass:*"
	},
	"dependencies": {
		"gulp": "^4.0.0",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.3",
		"gulp-myth": "^2.0.0",
		"gulp-sass": "^4.0.2",
		"gulp-uglify": "^3.0.1"
	}
}
